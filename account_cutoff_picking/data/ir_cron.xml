<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">

    <record forcecreate="True" id="ir_cron_cutoff_expense" model="ir.cron">
        <field name="name">Make cutoff at end of period - expense</field>
        <field eval="True" name="active" />
        <field name="model_id" ref="account_cutoff_base.model_account_cutoff" />
        <field name="state">code</field>
        <field name="code">model._cron_cutoff_expense()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">months</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-01 00:00:00')" />

    </record>

    <record forcecreate="True" id="ir_cron_cutoff_revenue" model="ir.cron">
        <field name="name">Make cutoff at end of period - revenue</field>
        <field eval="True" name="active" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">months</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field name="model_id" ref="account_cutoff_base.model_account_cutoff" />
        <field name="code">model._cron_cutoff_revenue()</field>
        <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-01 00:00:00')" />
    </record>

</odoo>
